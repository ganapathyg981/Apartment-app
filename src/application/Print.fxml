<?import javafx.scene.text.Font?>

<BorderPane xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.PrintCon">
   <center>
      <AnchorPane prefHeight="576.0" prefWidth="1024.0" style="-fx-background-color: #021423;" BorderPane.alignment="CENTER">
         <children>
            <ImageView fitHeight="576.0" fitWidth="1024.0" opacity="0.15" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@Images/wide_xlrg_view-of-tump-from-river.jpg" />
               </image>
            </ImageView>
            <DatePicker fx:id="fromp" layoutX="457.0" layoutY="178.0" scaleX="1.3" scaleY="1.3" style="-fx-opacity: .8;" />
            <DatePicker fx:id="top" layoutX="455.0" layoutY="277.0" scaleX="1.3" scaleY="1.3" style="-fx-opacity: .8;" />
            <Button layoutX="496.0" layoutY="342.0" mnemonicParsing="false" onAction="#Print" style="-fx-background-color: #e53935;;" text="Print" textFill="WHITE">
               <font>
                  <Font size="23.0" />
               </font>
            </Button>
            <Label layoutX="355.0" layoutY="172.0" text="From" textFill="WHITE">
               <font>
                  <Font size="23.0" />
               </font>
            </Label>
            <Label layoutX="360.0" layoutY="272.0" text="To" textFill="WHITE">
               <font>
                  <Font size="23.0" />
               </font>
            </Label>
            <Button layoutX="884.0" layoutY="503.0" mnemonicParsing="false" onAction="#back" style="-fx-background-color: #e53935;;" text="Back" textFill="WHITE">
               <font>
                  <Font size="19.0" />
               </font>
            </Button>
         </children>
      </AnchorPane>
   </center>
</BorderPane>
